{
	"geode": "4.2.0",
	"gd": {
		"win": "2.2074",
		"mac": "2.2074",
		"android": "2.2074"
	},
	"version": "v0.3.2-alpha",
	"id": "cps.kampwski.level_tags",
	"name": "CPS Level Tags",
	"developer": "KampWski",
	"description": "A mod that adds tags to levels.",
	"dependencies": [
		{
			"id": "geode.node-ids",
			"version": "v1.17.0",
			"importance": "required"
		}
	],
	"tags": [
		"interface",
		"utility",
		"online"
	],
	"resources": {
		"sprites": [
			"resources/*.png",
			"resources/tags/*.png"
		],
		"spritesheets": {
			"tags": [
				"resources/tags/*.png"
			]
		}
	},
	"links": {
		"source": "https://github.com/KampWskiR/Level-Tags",
		"community": "https://discord.gg/6GXYHf9WTB"
	},
	"settings": {
		"customization": {
            "name": "Customization",
            "type": "title",
            "description": "These are settings that will customize your experience as best as possible."
        },
		"levelcellShow": {
			"name": "LevelCell Tags",
			"description": "Enable Tags displaying in LevelCell",
			"type": "bool",
			"default": true
		},
		"levelInfoShow": {
			"name": "LevelInfoLayer Tags",
			"description": "Enable tags displaying in LevelInfoLayer",
			"type": "bool",
			"default": false
		},
		"pauseLayerShow": {
			"name": "Pause Screen Tags",
			"description": "Enable tags displaying in PauseLayer.",
			"type": "bool",
			"default": false
		},
		"hideRequest": {
			"name": "Hide Request Button",
			"type": "bool",
			"default": false
		},
		"tags-order": {
			"name": "Tag Order",
			"type": "string",
			"default": "style, theme, meta, gameplay",
			"one-of": ["style, theme, meta, gameplay","meta, gameplay, style, theme","gameplay, meta, style, theme"]
		},
		"experiments": {
            "name": "Experiments",
            "type": "title",
            "description": "These are unfinished features that may not work as expected. here be dragons."
        },
		"beta-search": {
			"name": "Beta Search",
			"description": "Enable tag searching (experiment).",
			"type": "bool",
			"default": false
		}
	}
}
